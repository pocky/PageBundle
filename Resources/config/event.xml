<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_page.infrastructure.domain_event.subscriber.created.class">%black_page.infrastructure.domain_event_subscriber.class.created%</parameter>
        <parameter key="black_page.infrastructure.domain_event.subscriber.writed.class">%black_page.infrastructure.domain_event_subscriber.class.writed%</parameter>
        <parameter key="black_page.infrastructure.domain_event.subscriber.published.class">%black_page.infrastructure.domain_event_subscriber.class.published%</parameter>
        <parameter key="black_page.infrastructure.domain_event.subscriber.depublished.class">%black_page.infrastructure.domain_event_subscriber.class.depublished%</parameter>
        <parameter key="black_page.infrastructure.domain_event.subscriber.removed.class">%black_page.infrastructure.domain_event_subscriber.class.removed%</parameter>
    </parameters>

    <services>
        <service id="black_page.infrastructure.domain_event.subscriber.created"
                 class="%black_page.infrastructure.domain_event.subscriber.created.class%">
            <argument type="service" id="logger" />
        </service>

        <service id="black_page.infrastructure.domain_event.subscriber.writed"
                 class="%black_page.infrastructure.domain_event.subscriber.writed.class%">
            <argument type="service" id="logger" />
        </service>

        <service id="black_page.infrastructure.domain_event.subscriber.published"
                 class="%black_page.infrastructure.domain_event.subscriber.published.class%">
            <argument type="service" id="logger" />
        </service>

        <service id="black_page.infrastructure.domain_event.subscriber.depublished"
                 class="%black_page.infrastructure.domain_event.subscriber.depublished.class%">
            <argument type="service" id="logger" />
        </service>

        <service id="black_page.infrastructure.domain_event.subscriber.removed"
                 class="%black_page.infrastructure.domain_event.subscriber.removed.class%">
            <argument type="service" id="logger" />
        </service>
    </services>
</container>
