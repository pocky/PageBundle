<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_page.manager.page">%black_page.page.manager%</parameter>
    </parameters>

    <services>
        <service id="black_page.document.manager" alias="doctrine.odm.mongodb.document_manager" />

        <service id="black_page.manager.page" class="%black_page.manager.page%">
            <argument type="service" id="black_page.document.manager" />
            <argument>%black_page.page.model.class%</argument>
        </service>
    </services>
</container>
