<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_page.infrastructure.cqrs.handler.create_web_page.class">Black\Bundle\PageBundle\Infrastructure\CQRS\Handler\CreateWebPageHandler</parameter>
    </parameters>

    <services>

        <service id="black_page.infrastructure.cqrs.handler.create_web_page" class="%black_page.infrastructure.cqrs.handler.create_web_page.class%">
            <argument type="service" id="black_page.infrastructure.service.write" />
            <argument type="service" id="black_page.manager.webpage" />
            <argument type="service" id="event_dispatcher" />
            <argument type="service" id="black_page.infrastructure.domain_event.event.subscriber" />
        </service>

    </services>
</container>
