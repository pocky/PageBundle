{% for document in documents %}
    <li>
        <a href="{{ url('page_show', { slug: document.slug }, true) }}" title="{% trans with {'%name%': document.name }%}page.menu.title{% endtrans %}">
            {{ document.name }}
        </a>
    </li>
{% else %}
    {% trans %}page.show.menu.error{% endtrans %}
{% endfor %}