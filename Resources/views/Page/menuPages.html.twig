{% for document in documents %}
    <li>
        <a href="{{ url('page_show', { slug: document.slug }, true) }}" title="{% trans with {'%name%': document.name } from "views" %}page.www.menuPages.content.a.title{% endtrans %}">
            {{ document.name }}
        </a>
    </li>
{% else %}
    {% trans from "views" %}page.menuPages.error.text{% endtrans %}
{% endfor %}